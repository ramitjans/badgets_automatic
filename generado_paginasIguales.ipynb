   "metadata": {},
   "outputs": [],
   "source": [
    "reader = PdfReader(\"output/005__acreditacionAttendee.pdf\")\n",
    "writer = PdfWriter()\n",
    "\n",
    "page = reader.pages[0]\n",
    "\n",
    "for _ in range(20):\n",
    "    writer.add_page(page)\n",
    "\n",
    "with open(\"output/output_20_paginas.pdf\", \"wb\") as f:\n",
    "    writer.write(f)"
   ]
  }
